{
    "manifest_version" : 2,
    "name" : "Weather",
    "description" : "Weather Extension",
    "version" : "1.0",
    "browser_action":{
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          },
      "default_popup": "popup.html"
    },
    "permissions" : [
        "geolocation",
        "storage"
    ],
    "background" : {
        "scripts" : ["background.js"]
    }
}